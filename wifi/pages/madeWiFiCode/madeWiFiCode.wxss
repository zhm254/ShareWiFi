page {
  background-color: #f5f5f5;
}

.wifi-list {
  padding-bottom: 20rpx;
}

.wifi-item {
  display: flex;
  background-color: white;
  margin: 20rpx 20rpx 0 20rpx;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 20rpx;
}

.wifi-code {
  width: 280rpx;
  height: 280rpx;
}

.wifi-name {
  word-break: break-all;
}

.explain-text {
  text-align: center;
  margin-top: 80rpx;
}
